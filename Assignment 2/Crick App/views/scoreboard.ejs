<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Scoreboard</title>
</head>
<body>
    <h1>Scoreboard</h1>
    <ul>
        <% matches.forEach(function(match) { %>
            <li><%= match.name %> - Status: <%= match.status %></li>
        <% }) %>
    </ul>
</body>
</html>
